{
	"functions-have-doc-strings": false,
	"variable-names": "camel_case",
	"function-names": "pascal_case",
	"readme-file": false,
	"readme-stats": false,
	"side-comments": false,
	"print-f-new-line": true,
	"only-show-errors": true,
	"ignore-if-in-comments": true,
	"list-internal-functions": false,
	"exported-identifiers-have-comments": true,
	"const-in-caps": true,
	"no-naked-returns": true
}
